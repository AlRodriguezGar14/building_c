#include <stdio.h>
#include <stdlib.h>
#include "libft.h"

char *ft_itoa(int n)
{
    char *out = NULL;
    size_t len = 0;


    out = (char*)malloc(4 * sizeof(char));
    out += 3;
    *out = '1';
    out--;
    *out = '2';
    out--;
    *out = '3';
    out--;
    *out = '4';

    printf("o: %s\n", out);
    return out;
}

int main()
{
    char *test  = ft_itoa(123313);
    printf("tests: %s\n", test);
    return (0);
}
